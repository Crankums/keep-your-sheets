<%# name %>
<%# username %>
<%# email %>
<%# handles* %>
<%# password %>
<%# age, city, state* %>
<%# bio/headline/catch phrase* %>
<%# submit %>

<form action='/user' method="POST">
    Name: <input type='text' name='name' id='name'><br><br>
    Username: <input type='text' name='username' id='username'><br><br>
    Email: <input type='text' name='email' id='email'><br><br>
    Password: <input type='text' name='password' id='password'><br><br>
    <%# confirm password %>
    Age: <input type='text' name='age' id='age'><br><br>
    City: <input type='text' name='city' id='city'><br><br>
    State: <input type='text' name='state' id='state'><br><br>
    Tell us a little about yourself, and put any of your other handles (Twitter, Instagram, Twitch, etc) here: <br><textarea name="bio" id="bio"></textarea><br><br>
    <input type="submit" value="Create New User">
</form>